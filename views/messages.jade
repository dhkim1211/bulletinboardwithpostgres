extends layout

block content
	div.col-md-12
		h2 Messages:
			br
			br
			each post in posts
				p.messages= "Title: " 
					em #{post.title} 
				p.messages= "Message: " 
					em #{post.body}
				//a.btn.btn-xs.btn-danger Delete
				br
				br

script( src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js' )
script( src='//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js' )